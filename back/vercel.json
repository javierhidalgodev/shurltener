{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ]
  ,
  "routes": [
    { 
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}